{
  "name": "tuture",
  "version": "1.5.0",
  "description": "Tuture magically turns your repositories into juicy tutorials.",
  "main": "index.ts",
  "preferGlobal": true,
  "keywords": [
    "learn-by-doing",
    "learn-by-examples",
    "hands-on",
    "tutorial",
    "diff-viewer",
    "git"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tslint -p . -t verbose",
    "test": "rimraf build && tsc && jest",
    "build": "rimraf build && tsc && cp -r editor/build build/editor",
    "pkg": "./scripts/pack.sh",
    "prepack": "rimraf dist && tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tutureproject/tuture.git"
  },
  "license": "MIT",
  "bin": {
    "tuture": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/tutureproject/tuture/issues"
  },
  "homepage": "https://github.com/tutureproject/tuture#readme",
  "files": [
    "/bin",
    "/build"
  ],
  "dependencies": {
    "@oclif/command": "^1.4.36",
    "@oclif/config": "^1.6.33",
    "@oclif/plugin-help": "^2.0.5",
    "chalk": "^2.4.1",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "globby": "^8.0.1",
    "graphql-request": "^1.8.2",
    "inquirer": "^6.0.0",
    "js-yaml": "^3.12.0",
    "micromatch": "^3.1.10",
    "morgan": "^1.9.1",
    "multer": "^1.3.1",
    "opn": "^5.4.0",
    "parse-diff": "^0.4.2",
    "request": "^2.88.0",
    "socket.io": "^2.1.1",
    "tmp": "^0.0.33",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/inquirer": "^0.0.42",
    "@types/jest": "^23.3.1",
    "@types/js-yaml": "^3.11.2",
    "@types/micromatch": "^3.1.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/opn": "^5.1.0",
    "@types/request": "^2.47.1",
    "@types/socket.io": "^1.4.38",
    "@types/tmp": "^0.0.33",
    "@types/which": "^1.3.1",
    "jest": "^23.5.0",
    "pkg": "^4.3.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "types": "lib/index.d.ts",
  "oclif": {
    "commands": "./build/commands",
    "bin": "tuture",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testURL": "http://localhost/",
    "verbose": true
  },
  "pkg": {
    "scripts": "bin/run",
    "assets": "build/**/*"
  }
}
